{
  "version": 2,
  "builds": [
    {
      "src": "seo-tool/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "seo-tool/$1"
    }
  ],
  "buildCommand": "cd seo-tool && npm install && npm run build",
  "outputDirectory": "seo-tool/.next"
}